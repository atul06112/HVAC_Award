<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.android.kumaratul.hvac_award.Sharec">

    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginEnd="107dp"
        android:layout_marginTop="22dp"
        android:text="CALL US"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/call"
        android:layout_marginEnd="19dp"
        android:layout_alignBottom="@+id/button16"
        android:layout_toStartOf="@+id/button16" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="44dp"
        app:srcCompat="@android:drawable/sym_action_chat"
        android:layout_below="@+id/editText24"
        android:layout_alignStart="@+id/imageView4" />

    <EditText
        android:id="@+id/editText24"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:ems="10"
        android:inputType="textMultiLine"
        android:text=""
        android:layout_below="@+id/imageView4"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/button17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MESSAGE US"
        android:layout_alignTop="@+id/imageView5"
        android:layout_alignEnd="@+id/button16" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/imageView5"
        android:layout_marginBottom="91dp"
        app:srcCompat="@android:drawable/sym_action_email" />

    <Button
        android:id="@+id/button18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EMAIL US"
        android:layout_alignBottom="@+id/imageView6"
        android:layout_alignEnd="@+id/button17" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:text="Mail Us:-priya.uit96@gmail.com"
        android:layout_below="@+id/button18"
        android:layout_alignParentStart="true"
        android:layout_marginStart="36dp"
        android:layout_marginTop="18dp" />

</RelativeLayout>
