<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.android.kumaratul.hvac_award.Projects">

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:text="INTERESTED IN PARTICIPATING IN AWARDS(YES/NO)"
        android:textSize="25dp"/>

    <EditText
        android:id="@+id/editText22"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView25"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="" />

    <Button
        android:id="@+id/button14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="86dp"
        android:text="SUBMIT"
        android:layout_below="@+id/textView25"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button14"
        android:layout_marginTop="41dp"
        android:text="INTERESTED IN RATING IN AWARDS(YES/NO)"
        android:textSize="25dp"/>

    <EditText
        android:id="@+id/editText23"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView29"
        android:layout_marginTop="19dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="" />

    <Button
        android:id="@+id/button15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SUBMIT"
        android:layout_alignBottom="@+id/editText23"
        android:layout_alignStart="@+id/button14" />
</RelativeLayout>
